{
  "name": "Simple DOM Editor",
  "description": "Small plugin for editing DOM in client-side",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["https://www.propertyguru.com.sg/new-project-launch*"],
    "js": ["utils.js", "views.js", "storage.js", "main.js"]
  }],
  "permissions": ["storage", "activeTab", "scripting"],
  "icons": {
    "16": "/logos/get_started16.png",
    "32": "/logos/get_started32.png",
    "48": "/logos/get_started48.png",
    "128": "/logos/get_started128.png"
  }
}
