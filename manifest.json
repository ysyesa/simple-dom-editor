{
  "name": "Simple DOM Editor",
  "description": "Small plugin for editing DOM in client-side",
  "version": "0.1",
  "manifest_version": 3,
  "content_scripts": [{
    "matches": ["https://agentnet.propertyguru.com.sg/*"],
    "js": [
      "developer_listing.js", 
      "developer_listing_tab_1.js", "developer_listing_tab_2.js", "developer_listing_tab_3.js",
      "developer_listing_tab_2_make_offer.js", "developer_listing_tab_3_completed.js",
      "main.js"
    ]
  }],
  "permissions": ["storage", "activeTab", "scripting"],
  "icons": {
    "16": "/logos/get_started16.png",
    "32": "/logos/get_started32.png",
    "48": "/logos/get_started48.png",
    "128": "/logos/get_started128.png"
  }
}
